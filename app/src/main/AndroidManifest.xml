<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.mobilemanager.wzq.mobilemanager"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <application
        android:name=".application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.IndexActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity">
        </activity>
        <activity android:name=".activity.SettingActivity">
        </activity>
        <activity android:name=".activity.PhoneGuardActivity"
            android:launchMode="singleTask">
        </activity>
        <activity
            android:name=".activity.PGsetup1Activity"
            android:launchMode="singleTask">
        </activity>
        <activity
            android:name=".activity.PGsetup2Activity"
            android:launchMode="singleTask">
        </activity>
        <activity
            android:name=".activity.PGsetup3Activity"
            android:launchMode="singleTask">
        </activity>
        <activity android:name=".activity.PGsetup4Activity"
            android:launchMode="singleTask">
        </activity>
    </application>

</manifest>
